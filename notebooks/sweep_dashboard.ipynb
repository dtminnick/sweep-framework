
# Cell 1: Imports
from sweep_framework.registry import SweepRegistry
from sweep_framework.analysis.sweep_analyzer import SweepAnalyzer
from sweep_framework.analysis.report_builder import ReportBuilder
import pandas as pd
import seaborn as sns
import matplotlib.pyplot as plt

# Cell 2: Load registry and build analyzer
registry = SweepRegistry()
analyzer = SweepAnalyzer(registry)
report = ReportBuilder(analyzer)

# Cell 3: View top configs
metric = "macro_f1"
top_k = 5
summary_df = report.build_summary(metric=metric, top_k=top_k)
summary_df

# Cell 4: Visualize trends
param = "dropout"
trend_df = report.build_trends(param=param, metric=metric)

plt.figure(figsize=(8, 4))
sns.barplot(data=trend_df, x=param, y=metric)
plt.title(f"{param} vs {metric}")
plt.show()

# Cell 5: Compare groups (optional)
group_comparison = analyzer.compare_groups("baseline", "regularized", metric=metric)
group_comparison
